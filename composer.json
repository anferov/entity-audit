{
    "name": "simplethings/entity-audit",
    "license": "LGPL-2.1",
    "keywords": [
        "Persistence",
        "Database",
        "Audit"
    ],
    "description": "Audit for Doctrine Entities",
    "require": {
        "php": "^5.6|~7.0",
        "doctrine/dbal": "~2.5",
        "doctrine/orm": "~2.5"
    },
    "require-dev": {
        "gedmo/doctrine-extensions": "^2.3.1",
        "phpunit/phpunit": "^5.5",
        "symfony/var-dumper": "~2.8|~3.0",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "conflict": {
        "gedmo/doctrine-extensions": "<2.3.1"
    },
    "autoload": {
        "psr-4": {
            "SimpleThings\\EntityAudit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleThings\\EntityAudit\\Tests\\": "tests/"
        }
    }
}
